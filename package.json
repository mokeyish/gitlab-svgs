{
  "name": "@gitlab/svgs",
  "version": "1.183.0",
  "description": "SVG Assets for GitLab",
  "main": "",
  "repository": "git@gitlab.com:gitlab-org/gitlab-svgs.git",
  "author": "GitLab",
  "private": false,
  "license": "MIT",
  "scripts": {
    "svg": "node build_scripts/index.js",
    "dev": "PORT=3333 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {},
  "devDependencies": {
    "@gitlab/eslint-plugin": "8.1.0",
    "bootstrap": "^4.3.1",
    "eslint": "7.20.0",
    "eslint-loader": "4.0.2",
    "glob": "^7.1.3",
    "husky": "^1.1.2",
    "mkdirp": "^0.5.1",
    "nuxt": "^2.2.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^2.6.2",
    "svg-sprite": "^1.5.0",
    "svg4everybody": "^2.1.9",
    "svgo": "^1.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --branch 'main' && npm run lint"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/exec",
        {
          "analyzeCommitsCmd": "./build_scripts/analyze-commits.sh ${lastRelease.gitTag}",
          "generateNotesCmd": "./build_scripts/generate-notes.sh ${lastRelease.gitTag} ${nextRelease.version}"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/gitlab"
    ],
    "success": false,
    "fail": false
  }
}
